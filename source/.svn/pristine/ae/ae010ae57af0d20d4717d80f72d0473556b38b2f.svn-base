        var back = function () {
            document.getElementById("list-details").style.display = "block";
            $('#divDetails').dialog('removebutton', 'Back');
            $('#divDetails').dialog('removebutton', 'Save');
            $('#divDetails').dialog('removebutton', 'Update');
            $('#divDetails').dialog('addbutton', 'Add Data', addData);
            $('#divDetails').dialog('addbutton', 'Copy Template', copyData);
        }

