        var opt1 = document.createElement("option");
        document.getElementById("utxtMIN_MATERIAL").options.add(opt1);
        opt1.text = data.MIN_MATERIAL;
        opt1.value = data.MIN_MATERIAL;
        for (ii = 0; ii < min.length - 1; ii++) {
            d = min[ii].split('|');
            var opt = document.createElement("option");
            document.getElementById("utxtMIN_MATERIAL").options.add(opt);
            opt.text = d[0];
            opt.value = d[0];
            countBtnEditMin++;
        }
    }

    function GetDetailsCallBackCopy(data) {
        $ = globalJquery;
        //alert(data.MIN_MATERIAL);
        SetElement("ctxtRACK_NO", data.RACK_NO);
        SetElement("ctxtFRAME_NO", data.FRAME_NO);
