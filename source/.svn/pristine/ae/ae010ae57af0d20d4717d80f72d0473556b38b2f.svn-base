<br />
<h2>Equipment Template List</h2>
<div style="height:650px">
    <div>
        <table style="width:850px;">
            <tr>
                <td>MANUFACTURER :</td>
                <td>@Html.DropDownList("MAN", (IEnumerable<SelectListItem>)ViewBag.MANUFACTURER, new { style = "width:150px" })</td>
                <td>MODEL :</td>
                <td>@Html.DropDownList("MOD", (IEnumerable<SelectListItem>)ViewBag.MODEL, new { style = "width:150px" }) </td>
                <td>FNO :</td>
                <td>@Html.DropDownList("FNO", (IEnumerable<SelectListItem>)ViewBag.FNO, new { style = "width:150px" })</td>
                <td style="text-align:right"><input type="button" value="Filter" id="btnSearch" /></td>
            </tr>
            <tr>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td></td>
                <td style="text-align:right"><input type="button" value="Add New" id="btnNewJob" /></td>
            </tr>
        </table>
    </div><br />
@if (Model.Count() > 0)
{
    <table id="rounded-corner" summary="ExchangeMaintenance List" width="850px">
        <thead>
            <tr>
                <th scope="col">MANUFACTURER</th>
                <th scope="col">MODEL</th>
                <th scope="col">TEMPLATE NAME</th>            
                <th scope="col">FNO</th>             
            </tr>
        </thead>
        <tbody>
        @foreach (var res in Model)
        {
            <tr class="odd">
                <td id="gid" style="display:none;">@res.MODEL_ID</td>
                <td>@res.MANUFACTURER</td>
                <td>@res.TEMPLATE_MODEL</td>
                <td>@res.XL_FILE</td>
                <td>@res.MSAN_FNO</td>
            </tr>    
        }
        </tbody>
    </table>
    <div>
        Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber)
        of @Model.PageCount 
        &nbsp;
        @if (Model.HasPreviousPage)
        {
            @Html.ActionLink("<<", "TemplateList", new { MAN = ViewBag.MAN_1, MOD = ViewBag.MOD_1, FNO = ViewBag.FNO_1, page = 1 })
            @Html.Raw("&nbsp;");
            @Html.ActionLink("< Prev", "TemplateList", new { MAN = ViewBag.MAN_1, MOD = ViewBag.MOD_1, FNO = ViewBag.FNO_1, page = Model.PageNumber - 1 })
        }
        else
        {

        }
        &nbsp;
        @if (Model.HasNextPage)
        {
            @Html.ActionLink("Next >", "TemplateList", new { MAN = ViewBag.MAN_1, MOD = ViewBag.MOD_1, FNO = ViewBag.FNO_1, page = Model.PageNumber + 1 })
            @Html.Raw("&nbsp;");
            @Html.ActionLink(">>", "TemplateList", new { MAN = ViewBag.MAN_1, MOD = ViewBag.MOD_1, FNO = ViewBag.FNO_1, page = Model.PageCount })
        }
        else
        {

        }
        <br/><b> Total @Model.TotalItemCount records</b>
    </div>
}
else
{
        @:<br /><span style="font-size:12px;">No record found.</span> 
}
<br />
    <div id="divLoading" style="visibility:hidden"><img src="@Url.Content("~/Content/images/rel_interstitial_loading.gif")" alt="loading..." /></div>
    <div id="divResultText"></div>
    <br />
    <div id="divDetails" title="Details" style="padding-left:40px; padding-top:30px;">
