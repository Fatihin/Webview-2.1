@model PagedList.IPagedList<WebView.WebService._base.AgTemplate>
@{
    ViewBag.Title = "List";
}
<script type="text/javascript">
    function GetDetailsCallBack(data) {

        $ = globalJquery;
        SetText("txtJobNo", data.xlName);
        SetText("txtTempName", data.xlName);
        SetElement("txtID", data.id);
        SetElement("itxtID", data.id);
        SetElement("ctxtID", data.id);
        SetElement("txtNewTemp", data.xlName);

        document.getElementById("txtRACK_NO").value     = "";
        document.getElementById("txtFRAME_NO").value    = "";
        document.getElementById("txtSLOT_NO").value     = "";
        document.getElementById("txtSLOT_NIS").value    = "";
        document.getElementById("txtCARD_TYPE").value   = "";
        document.getElementById("txtCARD_MODEL").value  = "";
        document.getElementById("txtPORT_LO").value     = "";
        document.getElementById("txtPORT_HI").value     = "";
        document.getElementById("txtMIN_MATERIAL").value = "";

        var tblNE = document.getElementById('crtNERequest');
        for (var i = tblNE.rows.length - 1; i > 0; i--) {
            tblNE.deleteRow(i);
        }

        if (data.crtNE) {
            document.getElementById("McrtNERequest").style.display = "block";
            document.getElementById("crtNERequest").style.display = "block";
            big = data.crtNE.split('!');
            countBtnEdit = 0;
            for (ii = 0; ii < big.length - 1; ii++) {
                d = big[ii].split('|');
                var tbl = document.getElementById('crtNERequest');
                var lastRow = tbl.rows.length;
                var iteration = lastRow;
                var row = tbl.insertRow(lastRow);
                row.style.background = "#e8eef4";
                row.style.textAlign = "Center";

                row.insertCell(0);
                row.insertCell(1);
                row.insertCell(2);
                row.insertCell(3);
                row.insertCell(4);
                row.insertCell(5);
                row.insertCell(6);
                row.insertCell(7);
                row.insertCell(8);
                row.insertCell(9);
                //alert(d[9]);
                if (d[0] == "") {d[0] = "-";}
                tbl.rows[ii + 1].cells[0].innerHTML = d[0]; tbl.rows[ii + 1].cells[0].width = "100px";
                tbl.rows[ii + 1].cells[1].innerHTML = d[1]; tbl.rows[ii + 1].cells[1].width = "100px";
                tbl.rows[ii + 1].cells[2].innerHTML = d[2]; tbl.rows[ii + 1].cells[2].width = "100px";
                tbl.rows[ii + 1].cells[3].innerHTML = d[3]; tbl.rows[ii + 1].cells[3].width = "100px";
                tbl.rows[ii + 1].cells[4].innerHTML = d[4]; tbl.rows[ii + 1].cells[4].width = "100px";
                tbl.rows[ii + 1].cells[5].innerHTML = d[5]; tbl.rows[ii + 1].cells[5].width = "150px";
                tbl.rows[ii + 1].cells[6].innerHTML = d[6]; tbl.rows[ii + 1].cells[6].width = "100px";
                tbl.rows[ii + 1].cells[7].innerHTML = d[7]; tbl.rows[ii + 1].cells[7].width = "100px";
                tbl.rows[ii + 1].cells[8].innerHTML = d[8]; tbl.rows[ii + 1].cells[8].width = "150px";
                tbl.rows[ii + 1].cells[9].innerHTML = "<input type=\"button\" value=\"Edit\" id=\"btnEdit" + countBtnEdit + "\"  onclick=\"UpdateData('" + d[9] + "')\"  style=\"width:50px;\" />" +
                                                      "<input type=\"button\" value=\"Delete\" id=\"btnDel" + countBtnEdit + "\"  onclick=\"DelData('" + d[9] + "')\"  style=\"width:60px;\" />" +
                                                      "<input type=\"button\" value=\"Copy\" id=\"btnCopy" + countBtnEdit + "\"  onclick=\"CopyData('" + d[9] + "')\"  style=\"width:60px;\" />";
                tbl.rows[ii + 1].cells[9].width = "200px";
                $("#btnEdit" + countBtnEdit).button();
                $("#btnDel" + countBtnEdit).button();
                $("#btnCopy" + countBtnEdit).button();
                countBtnEdit++;                
            }
        }
        else {
            document.getElementById("McrtNERequest").style.display = "none";
            document.getElementById("crtNERequest").style.display = "none";
        }

        countBtnEditMin = 0;

        min = data.crtMin.split('!');
        $('#txtMIN_MATERIAL').find('option').remove();
        for (ii = 0; ii < min.length - 1; ii++) {
            d = min[ii].split('|');
            var opt = document.createElement("option");
            document.getElementById("txtMIN_MATERIAL").options.add(opt);
            opt.text = d[0];
            opt.value = d[0];
            countBtnEditMin++;
        }

    }

    function GetDetailsCallBackUpdate(data) {
        $ = globalJquery;
        SetElement("utxtID", data.id);
        SetElement("utxtRACK_NO", data.RACK_NO);
        SetElement("utxtFRAME_NO", data.FRAME_NO);
        SetElement("utxtSLOT_NO", data.SLOT_NO);
        SetElement("utxtSLOT_NIS", data.SLOT_NIS);
