        $("#ctxtMIN_MATERIAL").html(itemsJob);
        min = data.crtMin.split('!');
        for (ii = 0; ii < min.length - 1; ii++) {
            d = min[ii].split('|');
            var opt = document.createElement("option");
            document.getElementById("ctxtMIN_MATERIAL").options.add(opt);
            opt.text = d[0];
            opt.value = d[0];
            countBtnEditMin++;
        }
    }

