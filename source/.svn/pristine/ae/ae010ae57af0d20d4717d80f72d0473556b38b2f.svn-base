            $('#insert-details').dialog('addbutton', 'Save', save);
        }

        globalJquery('#divDetails').dialog('addbutton', 'Add Data', addData);

        $(function () {
            $('#btnSearch').click(function () {
                window.location = "TemplateList?MAN=" + document.getElementById("MAN").value + "&MOD=" + document.getElementById("MOD").value + "&FNO=" + document.getElementById("FNO").value; //Where List is the function in the controller.
            });

