using System;
using System.Collections.Generic;
using System.Linq;
using System.Web;
using System.Web.Mvc;
using System.ComponentModel.DataAnnotations;

namespace WebView.Models
{
    public class MileageRatesModel
    {
        public string GradeId { get; set; }

        [Required]
        [Display(Name = "Grade")]
        public string Grade { get; set; }

        [Display(Name = "Grade Description")]
        public string GradeDescription { get; set; }

        [Display(Name = "Low KM Level 1")]
        public int LowLev1 { get; set; }

        [Display(Name = "High KM Level 1")]
        public int HighLev1 { get; set; }

        [Display(Name = "KM Rate 1")]
        public decimal Rate1 { get; set; }

        [Display(Name = "Low KM Level 2")]
        public int LowLev2 { get; set; }

        [Display(Name = "High KM Level 2")]
        public int HighLev2 { get; set; }

        [Display(Name = "KM Rate 2")]
        public decimal Rate2 { get; set; }

        [Display(Name = "Low KM Level 3")]
        public int LowLev3 { get; set; }

        [Display(Name = "High KM Level 3")]
        public int HighLev3 { get; set; }

        [Display(Name = "KM Rate 3")]
        public decimal Rate3 { get; set; }

    }
}
