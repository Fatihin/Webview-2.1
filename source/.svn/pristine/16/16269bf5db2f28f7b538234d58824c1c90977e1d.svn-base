<script type="text/javascript">

    function GetDetailsCallBack(data) {
        //alert(data.id);
        $ = globalJquery;
        SetElement("LoadID", data.id);
        SetElement("job", data.job);
        SetElement("ZEXC", data.zexc);
        
        $("#pathType option[value='"+data.pathType+"']").attr("selected","selected");
        var pathType = $('#pathType').trigger("change");      
        
        alert("Click OK to continue");
        $("#equipA option[value='"+data.equipA+"']").attr("selected","selected");
        var equipA = $('#equipA').trigger("change");
        $("#equipZ option[value='"+data.equipZ+"']").attr("selected","selected");
        var equipZ = $('#equipZ').trigger("change");

        alert("Click OK to continue");
        $("#cardA option[value='"+data.acardname+"']").attr("selected","selected");
        $("#cardZ option[value='"+data.zcardname+"']").attr("selected","selected");
        $('#cardA').trigger("change");
        $('#cardZ').trigger("change");

        SetElement("bandwidth", data.bandwidth);
        SetElement("primary", data.primary);
        SetElement("status", data.status);
        SetElement("txtINode", data.ipnode);
        SetElement("txtIUplink", data.ipuplink);
        SetElement("txtIDownlink", data.ipdownlink);
        SetElement("txtBearer", data.bearerid);
        SetElement("txtMedia", data.bearerpath);
        SetElement("txtMNode", data.msenode);
        SetElement("txtMDownlink", data.msedownlink);
    }

    function chekboxA(data){
        $ = globalJquery;
        if (document.boxes.valueA.length == undefined){
                document.boxes.valueA.checked = true;
        }else{
            document.boxes.valueA[data - 1].checked = true;
        }
    }

    function chekboxZ(data){
        $ = globalJquery;
        if (document.boxes.valueZ.length == undefined){
                document.boxes.valueZ.checked = true;
        }else{
            document.boxes.valueZ[data - 1].checked = true;
        }
    }

