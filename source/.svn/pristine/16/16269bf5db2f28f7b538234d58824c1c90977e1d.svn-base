    function edit(val) {
        $ = globalJquery;
        var cmsg = confirm("Are you sure you want to edit data?");
        if (cmsg != 0){
            $.ajax({
                url: "/VIS_NRM/GetDetails",
                type: "POST",
                dataType: "html",
                data: {
                    "id": val
                },
                beforeSend: function () {
                },
                success: function (data) {
                    var res = jQuery.parseJSON(data);
                    GetDetailsCallBack(res);    
                    alert("Completed");
                    chekboxA(res.aport);
                    chekboxZ(res.zport);
                    document.getElementById('btnUpdate').style.display = "block";
                    document.getElementById('btnSave').style.display = "none";
                }
            });
        }
    }

    function deleted(val) {
        $ = globalJquery;
        var cmsg = confirm("Are you sure you want delete to  data?");
        if (cmsg != 0){
            $.ajax({
                url: "/VIS_NRM/DeleteDetails",
                type: "POST",
                dataType: "html",
                data: {
                    "id": val
                },
                beforeSend: function () {
                },
                success: function (data) {
                    var res = jQuery.parseJSON(data);
                    alert("Data Deleted");
                }
            });
        }
    }

    function ShowIt() {
        document.getElementById("divLoading").style.visibility = "visible";
    }

    function HideIt() {
        document.getElementById("divLoading").style.visibility = "hidden";
    }
</script>

