                    for (var i = 0; i < document.boxes.valueZ.length; i++){
                        if (document.boxes.valueZ[i].checked){
                            valZ = valZ + document.boxes.valueZ[i].value + "/";
                        }
                    }
                }
                return result = valA + "!" + valZ;     
