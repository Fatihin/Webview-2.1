@model WebView.Models.BOQ_JKH_NewJobModel
           
@{
    ViewBag.Title = "BOQ_JKH_NewJob";
}
<br />
<br />
<h2>New BOQ JKH</h2>
@using (Html.BeginForm())
{
    @Html.ValidationSummary(true, "Error. Please correct the errors and try again.");
    <table width="600px" cellpadding="0" cellspacing="0" border="0">      
        <tr>
            <td>EXC ABB</td>
            <td>
             @Html.TextBoxFor(m => m.x_EXC_ABB, new { style = "width:200px;" })
            </td>
        </tr>
        <tr>
            <td>PU Description</td>
            <td>
             @Html.TextBoxFor(m => m.x_PU_DESC, new { style = "width:200px;" })
            </td>
        </tr>
        <tr>
            <td>PU Ins Year</td>
            <td>
             @Html.TextBoxFor(m => m.x_YEAR_INSTALL, new { style = "width:200px;" })
            </td>
        </tr>
         <tr>
            <td>Sch Type</td>
            <td>
             @Html.TextBoxFor(m => m.x_SCH_TYPE, new { style = "width:200px;" })
            </td>
        </tr>
         <tr>
            <td>Sch No</td>
            <td>
            @Html.TextBoxFor(m => m.x_SCH_NO, new { style = "width:200px;" })
            </td>
        </tr>
         <tr>
            <td>Scheme Name</td>
            <td>
            @Html.TextBoxFor(m => m.x_SCHEME_NAME, new { style = "width:200px;" })
            </td>
        </tr>
         <tr>
            <td>PU ID</td>
            <td>
            @Html.TextBoxFor(m => m.x_PU_ID, new { style = "width:200px;" })
            </td>
        </tr>
         <tr>
            <td>BQ Material Price</td>
            <td>
            @Html.TextBoxFor(m => m.x_BQ_MAT_PRICE, new { style = "width:200px;" })
            </td>
        </tr>
         <tr>
            <td>BQ Install Price</td>
            <td>
             @Html.TextBoxFor(m => m.x_BQ_INSTALL_PRICE, new { style = "width:200px;" })
            </td>
        </tr>
        <tr>
            <td>PU Quantity</td>
            <td>
             @Html.TextBoxFor(m => m.x_PU_QTY, new { style = "width:200px;" })
            </td>
        </tr>                          
         <tr>
            <td>OLD MAT PRICE</td>
            <td>
             @Html.TextBoxFor(m => m.x_OLD_MAT_PR, new { style = "width:200px;" })
            </td>
        </tr> 
         <tr>
            <td>OLD INSTALL PRICE</td>
            <td>
             @Html.TextBoxFor(m => m.x_OLD_INSTALL_PR, new { style = "width:200px;" })
            </td>
        </tr> 
         <tr>
            <td>CONSTRUCT BY</td>
            <td>
             @Html.TextBoxFor(m => m.x_CONSTRUCT_BY, new { style = "width:200px;" })
            </td>
        </tr> 
         <tr>
            <td>RECVE QTY</td>
            <td>
             @Html.TextBoxFor(m => m.x_RECVR_QTY, new { style = "width:200px;" })
            </td>
        </tr> 
         <tr>
            <td>RATE INDICATOR</td>
            <td>
             @Html.TextBoxFor(m => m.x_RATE_INDICATOR, new { style = "width:200px;" })
            </td>
        </tr>    
    </table>
    <p>
        <input type="submit" value="Save" />
    </p>
}