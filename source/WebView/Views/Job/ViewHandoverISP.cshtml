<br />
<h3>
  Job Management > Handover For @ViewBag.SchemeName
</h3>
<div style="height:650px;" >
  <br />
  @if (Model.Count() > 0)
  {
  <table id="rounded-corner" summary="Job List" width="850px">
    <thead>
      <tr>
        <th scope="col" style="width:70px">ID</th>
        <th scope="col" style="width:150px">CREATED DATE</th>
        <th scope="col" style="width:100px">STATUS</th>
        <th scope="col"> DESCRIPTION</th>
      </tr>
    </thead>
    <tbody>
      @foreach (var res in Model)
      {
                <tr class="odd">
                  <td id="gid">@res.G3E_ID@*</td>
                  <td >@res.Scheme_Name*@ </td>
                  <td>@res.G3E_IDENTIFIER <input type="hidden" id="jobID" value="@ViewBag.SchemeName"></td>
                  <td>@res.G3E_DESCRIPTION</td>
                  <td>@res.G3E_STATE</td>
                </tr>    
      }
    </tbody>
  </table>
  <div>
    Page @(Model.PageCount < Model.PageNumber ? 0 : Model.PageNumber)
    of @Model.PageCount
    &nbsp;
    @if (Model.HasPreviousPage)
    {
        int firstpages = 1;
        int prevpages = Model.PageNumber - 1;
        <a href = "ViewHandoverISP?id=@ViewBag.searchKey&page=@firstpages&type=@ViewBag.type">
          <
          <
        </a>
        @Html.Raw("&nbsp;")
        <a href = "ViewHandoverISP?id=@ViewBag.searchKey&page=@prevpages&type=@ViewBag.type">
          < Prev
        </a>
    }
    else
    {
    }
    &nbsp;
    @if (Model.HasNextPage)
    {
        int pages = Model.PageNumber + 1;
        int lastpages = Model.PageCount;
        <a href = "ViewHandoverISP?id=@ViewBag.searchKey&page=@pages&type=@ViewBag.type">
          Next >
        </a>
        @Html.Raw("&nbsp;")
        <a href = "ViewHandoverISP?id=@ViewBag.searchKey&page=@lastpages&type=@ViewBag.type"> >> </a>
    }
    else
    {
    }
  </div>
  }
  else
  {
      @:<br />
      <span style="font-size:12px;">
        No record found.
      </span> 
  }
