                        document.getElementById("selectcheckbox").style.display = "block";
                        typetab = "tabAddCardSplit1";
                        var AddCardSplit = res.LoadOrCreate;
                        var checkboxname = "ac";
                        getDetailsCheckJob(AddCardSplit, typetab, checkboxname);
                    }

                    else if (t == "Delete Equipment" && res.LoadOrCreate != "") {
                        document.getElementById("tabDelEquip").style.display = "block";
                        document.getElementById("selectcheckbox").style.display = "block";
                        typetab = "tabDelEquip1";
                        var DelEquip = res.LoadOrCreate;
                        var checkboxname = "de";
                        getDetailsCheckJob(DelEquip, typetab, checkboxname);
                    }

                    else if (t == "Delete Path" && res.LoadOrCreate != "") {
                        document.getElementById("tabDelPath").style.display = "block";
                        document.getElementById("selectcheckbox").style.display = "block";
                        typetab = "tabDelPath1";
                        var DelPath = res.LoadOrCreate;
                        var checkboxname = "dp";
                        getDetailsCheckJob(DelPath, typetab, checkboxname);
                    }

                    else if (t == "Delete Service Boundary" && res.LoadOrCreate != "") {
                        document.getElementById("tabDelSB").style.display = "block";
                        document.getElementById("selectcheckbox").style.display = "block";
                        typetab = "tabDelSB1";
                        var DelSB = res.LoadOrCreate;
                        var checkboxname = "dsb";
                        getDetailsCheckJob(DelSB, typetab, checkboxname);
                    }

