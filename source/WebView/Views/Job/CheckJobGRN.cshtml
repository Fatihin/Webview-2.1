        $(function () {
            document.getElementById("loading").style.display = "none";
            $('#submToGrn').click(function () {
                $.ajax({
                    url: "/Job/GetStatus",
                    type: "POST",
                    dataType: "html",
                    data: {
                        "id": document.getElementById("jobID").value
                    },
                    beforeSend: function () {
                        document.getElementById("loading").style.display = "block";
                    },
                    success: function (data) {
                        var res = jQuery.parseJSON(data);
                        alert(res);
                        document.getElementById("loading").style.display = "none";
                        if (res.checkGRN != 0) {
                            alert("Integration process is running. Please try again in a while.");
                        }
                        else if (res.checkGRNISP != 0) {
                            alert("Integration process is running. Please try again in a while.");
                        }
                        else {
                            Handover();
                        }
                    }
                });
            });

            $("#submToGrn").button();
        });

        $('#btnSave').click(function () {
            var temp = document.getElementById("typeOfInteg").value;
            if (temp == "Load Site") {
                namechkbox = "ls";
            }
            else if (temp == "Load Equipment") {
                namechkbox = "le";
            }
            else if (temp == "Load Service Boundary Address") {
                namechkbox = "lsb";
            }
            else if (temp == "Add Card/Splitter") {
                namechkbox = "ac";
            }
            else if (temp == "Delete Equipment") {
                namechkbox = "de";
            }
            else if (temp == "Delete Path") {
                namechkbox = "dp";
            }
            else if (temp == "Delete Service Boundary") {
                namechkbox = "dsb";
            }
            else if (temp == "Load Path Consumer") {
                namechkbox = "lpc";

            }
            var flagY = $("input[name=" + namechkbox + "]:checked").map(
            function () {
                return this.value;
            }).get().join(",");
            //alert("checked" + flagY);

            var flagN = $("input[name=" + namechkbox + "]:not(:checked)").map(
            function () {
                return this.value;
            }).get().join(",");
            //alert("not checked" + flagN);

            //checkbox for dp//
            var updateY = $("input[name=dptambahan ]:checked").map(
            function () {
                return this.value;
            }).get().join(",");
            //alert("DP TAMBAHAN ---- checked" + updateY);

            var newN = $("input[name=dptambahan]:not(:checked)").map(
            function () {
                return this.value;
            }).get().join(",");
            //alert("DP TAMBAHAN ---- NOT checked" + newN);

            var cmsg = confirm('Are you sure you want to save?');
            if (cmsg != 0) {
                $.ajax({
                    url: "/Job/UpdateFlagInteg",
                    type: "POST",
                    dataType: "html",
                    data: {
                        "flagIntegY": flagY,
                        "flagIntegN": flagN,
                        "updateY": updateY,
                        "newN": newN,
                        "typeOfInte": document.getElementById("typeOfInteg").value
                    },
                    beforeSend: function () {
                        document.getElementById("loading").style.display = "block";
                    },
                    success: function (data) {
                        document.getElementById("loading").style.display = "none";
                        var res = jQuery.parseJSON(data);
                        if (res.Success == true && res.Success2 == true && res.Success3 == true && res.Success4 == true) {
                            alert("Successfully Updated.");
                            window.location.reload();
                        }
                        else {
                            alert("Fails");
                            window.location.reload();
                        }
                    }
                });
            }
        });
        $("#btnSave").button();

        function Handover() {
            var cmsg = confirm('Are you sure you want to Handover?');
            if (cmsg != 0) {

                $.ajax({
                    url: "/Job/GraniteApproval2",
                    type: "POST",
                    dataType: "html",
                    data: {
                        "targetJob": document.getElementById("jobID").value,
                        "JOBTYPE": document.getElementById("JOBTYPE").value
                    },
                    beforeSend: function () {
                        document.getElementById("loading").style.display = "block";
                    },
                    success: function (data) {
                        var res = jQuery.parseJSON(data);
                        document.getElementById("loading").style.display = "none";
                        if (res.result == "ok") {
                            alert("Handed Over.");
                            window.location.reload();
                        }
                        else {
                            alert("Handover Started");
                            window.location.reload();
                        }
                    }
                });
            }

        }

        $('#btnHandoverStatus').click(function () {
            var jobtype = document.getElementById("JOBTYPE").value;
            if (jobtype == 'OSP') {
                window.location = "ViewHandover?id=" + document.getElementById("jobID").value + "&type='Granite'";
            }
            else if (jobtype == 'ISP') {
                window.location = "ViewHandoverISP?id=" + document.getElementById("jobID").value + "&type='Granite'";
            }
        });
        $("#btnHandoverStatus").button();

        // region Mubin CR45-17052018
        $('#expToXLSX').click(function () {
            window.location = "GenerateXLSX?targetJob=" + document.getElementById("jobID").value;
        });
        $("#expToXLSX").button();
        // endregion

        $("#selectall").click(function () {
            $("input:checkbox:not(:disabled)").each(function () {
                $(this).prop('checked', true);
            })
        });
        $("#selectall").button();
        $("#deselectall").click(function () {
            $("input:checkbox").each(function () {
                $(this).prop('checked', false);
            })
        });
        $("#deselectall").button();
        //Fatihin CR53 16052018        
    });

    //Marfu'ah ASBGated 280519
    function checkValidation(id, ptt, g3e_fno, resp) {
        jQuery(document).ready(function ($) {
            $.ajax({
                url: "/Job/checkASBGated",
                type: "POST",
                async: false,
                dataType: "html",
                data: {
                    "ptt": ptt,
                    "g3e_fno": g3e_fno,
                    "id": id
                },
                success: function (data) {
                    var res = jQuery.parseJSON(data);
                    resp(res);  //callback method
                }
            });
        });
    }
    //Marfu'ah ASBGated 280519
</script>

