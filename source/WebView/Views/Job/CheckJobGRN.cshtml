                                row.insertCell(1);
                                row.insertCell(2);
                                row.insertCell(3);
                                row.insertCell(4);
                                row.insertCell(5);
                                row.insertCell(6);
                                row.insertCell(7);
                                row.insertCell(8);
                                row.insertCell(9);
                                row.insertCell(10);
                                row.insertCell(11);
                                row.insertCell(12);
                                row.insertCell(13);
                                row.insertCell(14);
                                row.insertCell(15);

                                if (d[1] == "Y") {
                                    tbl.rows[ii + 1].cells[0].innerHTML = "<input type=\"checkbox\" id=\"valueZ\" name=\"lpc\" value=\"" + d[0] + "\" checked/>";
                                }
                                else if (d[1] == "N") {
                                    tbl.rows[ii + 1].cells[0].innerHTML = "<input type=\"checkbox\" id=\"valueZ\" name=\"lpc\" value=\"" + d[0] + "\" />";
                                }

                                tbl.rows[ii + 1].cells[1].innerHTML = d[2];
                                tbl.rows[ii + 1].cells[2].innerHTML = d[3];
                                tbl.rows[ii + 1].cells[3].innerHTML = d[4];
                                tbl.rows[ii + 1].cells[4].innerHTML = d[5];
                                tbl.rows[ii + 1].cells[5].innerHTML = d[6];
                                tbl.rows[ii + 1].cells[6].innerHTML = d[7];
                                tbl.rows[ii + 1].cells[7].innerHTML = d[8];
                                tbl.rows[ii + 1].cells[8].innerHTML = d[9];
                                tbl.rows[ii + 1].cells[9].innerHTML = d[10];
                                tbl.rows[ii + 1].cells[10].innerHTML = d[11];
                                tbl.rows[ii + 1].cells[11].innerHTML = d[12];
                                tbl.rows[ii + 1].cells[12].innerHTML = d[13];
                                tbl.rows[ii + 1].cells[13].innerHTML = d[14];
                                tbl.rows[ii + 1].cells[14].innerHTML = d[15];

                                if (d[16] == "UPDATE") {
